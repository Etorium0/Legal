  "document": {
    "title": "Nghị định 100/2019/NĐ-CP về xử phạt vi phạm hành chính trong lĩnh vực giao thông đường bộ",
    "type": "decree",
    "number": "100/2019/NĐ-CP",
    "year": 2019,
    "authority": "Chính phủ"
  },
  "units": [
    {
      "level": "article",
      "code": "Art 6",
      "text": "Phạt tiền từ 800.000 đồng đến 1.000.000 đồng đối với người điều khiển xe mô tô, xe gắn máy thực hiện một trong các hành vi sau đây: a) Không đội mũ bảo hiểm cho người điều khiển xe hoặc để người được chở không đội mũ bảo hiểm; b) Đội mũ bảo hiểm không đúng quy cách, không cài quai đúng quy định; c) Chở quá số người quy định; d) Dùng ô, dù khi điều khiển xe.",
      "order_index": 1
    },
    {
      "level": "clause",
      "code": "Art 6 Cl a",
      "text": "Không đội mũ bảo hiểm cho người điều khiển xe hoặc để người được chở không đội mũ bảo hiểm",
      "parent_code": "Art 6",
      "order_index": 1
    },
    {
      "level": "clause", 
      "code": "Art 6 Cl d",
      "text": "Dùng ô, dù khi điều khiển xe",
      "parent_code": "Art 6",
      "order_index": 4
